{
    "name": "a替换指定请求",
    "manifest_version": 2,
    "version": "1.0.0",
    "description": "替换指定请求",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content_script.js"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "storage"
    ]
}